<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<Pane maxHeight="400" maxWidth="600" minHeight="400" minWidth="600"
	prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerHome">
	<children>
		<MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="610.0">
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#signOff"
							text="Sign Off" />
						<MenuItem mnemonicParsing="false" onAction="#closeSystem"
							text="Close" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Help">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#aboutMe"
							text="About" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<Label fx:id="headerLabelUser" layoutX="201.0" layoutY="48.0"
			text="Welcome, ɡrāSHəs">
			<font>
				<Font size="24.0" />
			</font>
		</Label>
		<Pagination fx:id="pagining" layoutX="-2.0" layoutY="95.0"
			prefHeight="306.0" prefWidth="606.0" stylesheets="@../application/application.css">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Pagination>
		<Label layoutX="100.0" layoutY="137.0" prefHeight="65.0"
			prefWidth="500.0" text="" textFill="#e50e0ec1" fx:id="showLabel">
			<font>
				<Font size="23.0" />
			</font>
		</Label>
	</children>
</Pane>
